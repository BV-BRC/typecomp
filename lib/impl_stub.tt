package [% impl_package_name %];
use strict;

=head1 NAME

[% module_name %]

=head1 DESCRIPTION

[% module_doc %]

=cut

#BEGIN_HEADER
[% module_header -%]
#END_HEADER

sub new
{
    my($class) = @_;
    my $self = {
    };
    #BEGIN_CONSTRUCTOR
[% module_constructor -%]
    #END_CONSTRUCTOR

    bless $self, $class;
    if ($self->can('_init_instance'))
    {
	$self->_init_instance();
    }
    return $self;
}

=head1 METHODS

[% FOREACH method IN methods %]

=head2 [% method.name %]

  [% IF method.ret_vars -%][% method.ret_vars -%] = [% END -%]$obj->[% method.name %]([% method.arg_vars %])

=over 4

=item Parameter and return types

=begin html

<pre>
[% FOREACH docline IN method.arg_doc -%]
[% docline %]
[% END %]
</pre>

=end html

=begin text

[% FOREACH docline IN method.arg_doc -%]
[% docline %]
[% END %]

=end text



=item Description

[% method.doc %]

=back

=cut

sub [% method.name %]
{
    my($self, $ctx
       [%- IF method.arg_count > 0 -%], [% method.arg_vars %][% END %]) = @_;
    my([% method.ret_vars %]);
    #BEGIN [% method.name %]
    [% method.user_code -%]
    #END [% method.name %]
    return([% method.ret_vars %]);
}


[% END %]

=head1 TYPES

[% FOREACH type IN types %]

=head2 [% type.name %]

=over 4

[% IF type.comment %]

